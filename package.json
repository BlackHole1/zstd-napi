{
  "name": "zstd-napi",
  "version": "0.0.3",
  "description": "zstd bindings with N-API",
  "repository": {
    "type": "git",
    "url": "https://github.com/drakedevel/zstd-napi.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "node-gyp configure && node-gyp build",
    "build-prebuild": "prebuild -r napi --all",
    "install": "prebuild-install -r napi || node-gyp rebuild",
    "test": "jest",
    "test-lcov": "jest --coverage --coverageReporters=lcovonly"
  },
  "author": "Andrew Drake <adrake@adrake.org>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "jest": "^25.1.0",
    "prebuild": "^10.0.0",
    "ts-jest": "^25.1.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@types/node": "*",
    "node-addon-api": "^2.0.0",
    "prebuild-install": "^5.3.3"
  },
  "binary": {
    "napi_versions": [
      3,
      5
    ]
  },
  "files": [
    "binding.d.ts",
    "binding.gyp",
    "binding.js",
    "deps",
    "dist",
    "src"
  ]
}
